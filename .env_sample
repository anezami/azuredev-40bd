# Azure AI Foundry Agent Configuration
# Rename this file to .env and fill in your actual values

# Azure AI Foundry Agent ID - required to connect to your specific agent
AZURE_EXISTING_AGENT_ID="your_agent_id_here"

# Azure environment name
AZURE_ENV_NAME="your_env_name_here"

# Azure region where resources are deployed
AZURE_LOCATION="eastus"

# Your Azure subscription ID
AZURE_SUBSCRIPTION_ID="your_subscription_id_here"

# Endpoint for the AI project (if applicable)
AZURE_EXISTING_AIPROJECT_ENDPOINT=""

# Resource ID for the Azure AI project
AZURE_EXISTING_AIPROJECT_RESOURCE_ID="/subscriptions/your_subscription_id/resourceGroups/your_resource_group/providers/Microsoft.MachineLearningServices/workspaces/your_workspace_name"

# Additional configuration flags
AZD_ALLOW_NON_EMPTY_FOLDER=true

# Connection string for AIProjectClient in format: region.api.azureml.ms;subscription_id;resource_group;workspace_name
# This is required for the run_agent.py script to connect to the Azure AI Foundry
AI_PROJECT_CONNECTION_STRING="your_region.api.azureml.ms;your_subscription_id;your_resource_group;your_workspace_name"
